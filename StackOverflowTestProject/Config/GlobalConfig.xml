<?xml version="1.0" encoding="utf-8" ?>
<StackOverflowAutoFramework>
  <RunSettings>
    <SIT>https://stackoverflow.com/</SIT>
    <!-- Possible flag Chrome,Firefox, IE-->
    <Browser>Chrome</Browser>
    <!-- Possible flag Smoke, Regression, Unit  -->
    <TestType>FunctionalTesting</TestType>
    <!-- Possible Flag Y, N -->
    <IsLog>Y</IsLog>
    <LogPath>C:\\StackOverflowAutoLogs\\</LogPath>
    <BuildName>DemoBuild</BuildName>
    <!-- Possible Flag Y, N -->
    <IsReport>N</IsReport>
    <UserName>StackOverflowAutoTeam</UserName>
    <!-- Possible flag Selenium, Specflow  -->
    <Technology>Selenium</Technology>
  </RunSettings>
</StackOverflowAutoFramework>
